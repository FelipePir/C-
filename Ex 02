#include <iostream>
using namespace std;

int main(){
	
	int n1, n2, n3, maior;
	
	cout << "Digite 3 numeros: ";
	cin >> n1 >> n2 >> n3;
	
	if(n1>n2 && n1>n3) {
		maior = n1;
	}
	
	else if(n2>n1 && n2>n3) {
		maior = n2;
	}
	
	else {
		maior = n3;
	}
	
	cout << "O maior numero e: " << maior;
	
	return 0;
}
